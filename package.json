{
  "name": "fs-equal",
  "version": "1.0.0",
  "description": "Check if files and folders are identical",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel lib -d dist",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-register test/**/*-test.js",
    "posttest": "npm run lint",
    "cover": "babel-node node_modules/isparta/bin/isparta cover --include-all-sources --report html --report lcov node_modules/mocha/bin/_mocha -- --compilers js:babel-register test/**/*-test.js",
    "debug": "node debug node_modules/mocha/bin/_mocha --no-timeouts --compilers js:babel-register test/**/*-test.js",
    "prelint": "eslint --version",
    "lint": "eslint lib \"test/!(fixtures)/**/*\"",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kellyselden/fs-equal.git"
  },
  "author": "Kelly Selden <kellyselden@gmail.com> (https://github.com/kellyselden)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kellyselden/fs-equal/issues"
  },
  "homepage": "https://github.com/kellyselden/fs-equal#readme",
  "dependencies": {
    "denodeify": "^1.0.0",
    "klaw": "^1.0.0",
    "promise": "^8.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-register": "^6.0.0",
    "chai": "^4.0.0",
    "coveralls": "^2.0.0",
    "eslint": "^3.0.0",
    "eslint-config-sane": "^0.1.0",
    "isparta": "^4.0.0",
    "mocha": "^3.0.0",
    "rimraf": "^2.0.0"
  },
  "keywords": [
    "fs",
    "fs-extra",
    "compare",
    "equal",
    "file",
    "dir"
  ],
  "engines": {
    "node": ">= 4"
  }
}
