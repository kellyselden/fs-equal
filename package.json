{
  "name": "fs-equal",
  "version": "0.0.2",
  "description": "Check if files and folders are identical",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel lib -d dist",
    "prepublish": "npm run build",
    "test": "eslint-mocha --eslint-args=\"lib/**/*.js test/!(fixtures)/**/*.js\" --mocha-args=\"--compilers js:babel-register test/**/*-test.js\"",
    "cover": "babel-node node_modules/isparta/bin/isparta cover --include-all-sources --report html --report lcov node_modules/eslint-mocha/bin/eslint-mocha.js -- --eslint-args=\"lib/**/*.js test/!(fixtures)/**/*.js\" --mocha-args=\"--compilers js:babel-register test/**/*-test.js\"",
    "debug": "node debug node_modules/mocha/bin/_mocha --no-timeouts --compilers js:babel-register test/**/*-test.js",
    "lint": "eslint lib/**/*.js test/!(fixtures)/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kellyselden/fs-equal.git"
  },
  "author": "Kelly Selden <kellyselden@gmail.com> (https://github.com/kellyselden)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kellyselden/fs-equal/issues"
  },
  "homepage": "https://github.com/kellyselden/fs-equal#readme",
  "dependencies": {
    "denodeify": "^1.0.0",
    "fs-extra": "0.26.4",
    "promise": "^7.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-register": "^6.0.0",
    "chai": "^3.0.0",
    "coveralls": "^2.0.0",
    "eslint-mocha": "0.0.4",
    "isparta": "^4.0.0"
  },
  "keywords": [
    "fs",
    "fs-extra",
    "compare",
    "equal",
    "file",
    "dir"
  ]
}
